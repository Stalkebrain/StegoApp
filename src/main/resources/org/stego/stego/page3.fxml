<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.shape.Rectangle?>

<Pane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.stego.stego.Page3Controller">
   <children>
      <!-- Рамка и изображение 1 -->
      <StackPane layoutX="100.0" layoutY="100.0" prefHeight="400.0" prefWidth="400.0">
         <Rectangle arcHeight="5.0" arcWidth="5.0" fill="TRANSPARENT" height="300.0" stroke="BLACK" strokeWidth="2.0" width="300.0" />
         <ImageView fx:id="imageView1" fitHeight="300.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
      </StackPane>
      <!-- Рамка и изображение 2 -->
      <StackPane layoutX="700.0" layoutY="100.0" prefHeight="400.0" prefWidth="400.0">
         <Rectangle arcHeight="5.0" arcWidth="5.0" fill="TRANSPARENT" height="300.0" stroke="BLACK" strokeWidth="2.0" width="300.0" />
         <ImageView fx:id="imageView2" fitHeight="300.0" fitWidth="300.0" pickOnBounds="true" preserveRatio="true" />
      </StackPane>
      <Button fx:id="button1" layoutX="500.0" layoutY="600.0" mnemonicParsing="false" onAction="#visualAttack" prefHeight="40.0" prefWidth="200.0" text="Визуальная атака" />
   </children>
</Pane>
